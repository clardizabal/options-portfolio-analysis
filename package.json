{
  "name": "optioin-portfolio-analysis",
  "version": "0.1.0",
  "description": "A project to to analyze my portfolio performance",
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "repository": "github.com/clardizabal/options-portfolio-analysis",
  "author": "clardizabal",
  "license": "MIT",
  "proxy": "http://localhost:3000",
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^22.1.1",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/node": "^12.12.14",
    "jest": "^22.1.4",
    "ts-jest": "^22.0.2",
    "typescript": "^2.7.1"
  },
  "scripts": {
    "build": "npm run build:server && npm run build:web",
    "build:web": "webpack --config ./webpack-front.config.js",
    "build:server": "tsc -p ./app",
    "test": "jest --env=node --colors --coverage test",
    "start": "node lib/app.js",
    "start:dev": "npm run build && npm run sass && npm run copy:html && npm run start",
    "copy:html": "find src -name '*.html' -type f -exec cp {} lib/src \\;",
    "sass": "sass src/style/main.scss lib/src/style/main.css"
  },
  "dependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.2",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.16.0",
    "request": "^2.88.0",
    "sass": "^1.23.7",
    "sequelize": "^5.15.1",
    "uuidv4": "^6.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
